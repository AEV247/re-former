<h1>Register</h1>
<%= form_with model: @user, method: :post  do |form| %>
<%= form.label :username%>
<%= form.text_field :username, placeholder: "Enter username..." %>
<%= form.label :password %>
<%= form.password_field :password %>
<%= form.label :email %>
<%= form.email_field :email %>
<%= form.submit :submit %>
<% end %>
